{
  "name": "@adonisjs/bodyparser",
  "version": "9.0.0-0",
  "description": "BodyParser middleware for AdonisJS http server to read and parse request body",
  "main": "build/index.js",
  "type": "module",
  "files": [
    "build/src",
    "build/test_factories",
    "build/index.d.ts",
    "build/index.js"
  ],
  "exports": {
    ".": "./build/index.js",
    "./test_factories/*": "./build/test_factories/*.js",
    "./types": "./build/src/types.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "cross-env NODE_DEBUG=adonisjs:bodyparser c8 npm run vscode:test",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "release": "np",
    "version": "npm run build",
    "format": "prettier --write .",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "sync-labels": "github-label-sync --labels .github/labels.json adonisjs/bodyparser",
    "vscode:test": "node --loader=ts-node/esm bin/test.ts"
  },
  "devDependencies": {
    "@adonisjs/application": "^6.7.0-0",
    "@adonisjs/encryption": "^5.1.0-0",
    "@adonisjs/events": "^8.4.2-0",
    "@adonisjs/fold": "^9.9.0-0",
    "@adonisjs/http-server": "^6.4.2-0",
    "@adonisjs/logger": "^5.2.0-0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@japa/assert": "^1.3.6",
    "@japa/run-failed-tests": "^1.1.0",
    "@japa/runner": "^2.2.2",
    "@japa/spec-reporter": "^1.3.2",
    "@poppinss/file-generator": "^2.0.1-0",
    "@swc/core": "^1.3.27",
    "@types/bytes": "^3.1.1",
    "@types/end-of-stream": "^1.4.1",
    "@types/fs-extra": "^11.0.1",
    "@types/media-typer": "^1.1.1",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "c8": "^7.12.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-adonis": "^3.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "github-label-sync": "^2.2.0",
    "husky": "^8.0.3",
    "np": "^7.6.3",
    "prettier": "^2.8.3",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@poppinss/co-body": "^1.1.3",
    "@poppinss/macroable": "^1.0.0-0",
    "@poppinss/multiparty": "^2.0.1",
    "@poppinss/utils": "^6.3.1-0",
    "bytes": "^3.1.2",
    "cuid": "^2.1.8",
    "end-of-stream": "^1.4.4",
    "file-type": "^18.2.0",
    "fs-extra": "^11.1.0",
    "media-typer": "^1.1.0"
  },
  "peerDependencies": {
    "@adonisjs/http-server": "^6.4.2-0"
  },
  "keywords": [
    "adonisjs",
    "bodyparser",
    "multipart"
  ],
  "license": "MIT",
  "author": "virk,adonisjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/bodyparser.git"
  },
  "bugs": {
    "url": "https://github.com/adonisjs/bodyparser/issues"
  },
  "homepage": "https://github.com/adonisjs/bodyparser#readme",
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build",
    "backup"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "next",
    "branch": "main",
    "anyBranch": false
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**",
      "test_factories/**",
      ".yalc/**"
    ]
  }
}
