{
  "name": "@adonisjs/bodyparser",
  "version": "5.1.0",
  "description": "AdonisJs body parser to read and parse HTTP request bodies",
  "main": "build/providers/BodyParserProvider.js",
  "files": [
    "build/src",
    "build/adonis-typings",
    "build/providers"
  ],
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node japaFile.js",
    "clean": "del build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json adonisjs/bodyparser"
  },
  "devDependencies": {
    "@adonisjs/application": "^4.0.2",
    "@adonisjs/encryption": "^4.0.0",
    "@adonisjs/http-server": "^5.1.0",
    "@adonisjs/mrm-preset": "^3.0.0",
    "@adonisjs/require-ts": "^2.0.2",
    "@poppinss/dev-utils": "^1.1.0",
    "@types/bytes": "^3.1.0",
    "@types/end-of-stream": "^1.4.0",
    "@types/fs-extra": "^9.0.7",
    "@types/media-typer": "^1.1.0",
    "@types/multiparty": "0.0.32",
    "@types/node": "^14.14.27",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-adonis": "^1.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "github-label-sync": "^2.0.0",
    "husky": "^5.0.9",
    "japa": "^3.1.1",
    "mrm": "^2.5.18",
    "np": "^7.4.0",
    "npm-audit-html": "^1.5.0",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "typescript": "^4.1.5"
  },
  "peerDependencies": {
    "@adonisjs/application": "^4.0.0",
    "@adonisjs/http-server": "^5.0.0"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js",
      "pre-commit": "npm audit --production --json | ./node_modules/.bin/npm-audit-html && git add npm-audit.html"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "dependencies": {
    "@poppinss/co-body": "^1.0.0",
    "@poppinss/utils": "^3.0.3",
    "bytes": "^3.1.0",
    "cuid": "^2.1.8",
    "end-of-stream": "^1.4.4",
    "file-type": "^16.2.0",
    "fs-extra": "^9.1.0",
    "media-typer": "^1.1.0",
    "multiparty": "^4.2.2"
  },
  "publishConfig": {
    "access": "public",
    "tag": "alpha"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-bodyparser.git"
  },
  "keywords": [
    "adonisjs",
    "bodyparser",
    "multipart"
  ],
  "author": "virk,adonisjs",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-bodyparser/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-bodyparser#readme"
}
